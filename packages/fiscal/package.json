{
  "name": "@akimeo/fiscal",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "dev": "tsc -w",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "preci:test": "pnpm exec playwright install",
    "ci:test": "pnpm run test",
    "lint": "eslint",
    "ci:lint": "pnpm run lint --format compact",
    "format": "pnpm run lint --fix && prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "dependencies": {
    "@akimeo/donnees-reglementaires": "workspace:*",
    "date-fns": "catalog:"
  },
  "devDependencies": {
    "@akimeo/eslint-config": "workspace:*",
    "@akimeo/modele": "workspace:*",
    "@akimeo/prettier-config": "workspace:*",
    "@akimeo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "playwright": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@akimeo/prettier-config"
}
