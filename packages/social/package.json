{
  "private": true,
  "name": "@akimeo/social",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "build": "tsc && tsc-alias",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write"
  },
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "dependencies": {
    "@akimeo/modele": "workspace:*",
    "@akimeo/fiscal": "workspace:*",
    "@akimeo/donnees-reglementaires": "workspace:*",
    "es-toolkit": "catalog:",
    "modele-social": "catalog:",
    "publicodes": "catalog:"
  },
  "devDependencies": {
    "@akimeo/eslint-config": "workspace:*",
    "@akimeo/prettier-config": "workspace:*",
    "@akimeo/typescript-config": "workspace:*",
    "concurrently": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsc-alias": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@akimeo/prettier-config"
}
