{
  "name": "@akimeo/embed",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json && tsc-alias -p ./tsconfig.build.json",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "ci:lint": "pnpm run lint --format compact",
    "format": "pnpm run lint --fix && prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@akimeo/eslint-config": "workspace:*",
    "@akimeo/prettier-config": "workspace:*",
    "@akimeo/typescript-config": "workspace:*",
    "@types/react": "catalog:",
    "concurrently": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "tsc-alias": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@types/react": "catalog:",
    "react": "catalog:"
  },
  "prettier": "@akimeo/prettier-config"
}
