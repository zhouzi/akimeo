{
  "private": true,
  "name": "simulateurs",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --port 3001\" \"pnpm run build:iframe --watch\"",
    "start": "vite",
    "typecheck": "tsc",
    "prebuild": "pnpm run typecheck && pnpm run build:iframe",
    "build": "vite build",
    "build:iframe": "vite build --config vite.iframe.config.ts",
    "serve": "vite preview",
    "test": "vitest run --passWithNoTests",
    "ci:test": "pnpm run test",
    "lint": "eslint",
    "ci:lint": "pnpm run lint --format compact",
    "format": "pnpm run lint --fix && prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache"
  },
  "dependencies": {
    "@akimeo/embed": "workspace:*",
    "@akimeo/fiscal": "workspace:*",
    "@akimeo/modele": "workspace:*",
    "@akimeo/ui": "workspace:*",
    "@fontsource-variable/rethink-sans": "catalog:",
    "@fontsource-variable/saira": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@tanstack/react-form": "catalog:",
    "@tanstack/react-router": "catalog:",
    "@tanstack/router-plugin": "catalog:",
    "lucide-react": "catalog:",
    "plausible-tracker": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwindcss": "catalog:",
    "tw-animate-css": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@akimeo/eslint-config": "workspace:*",
    "@akimeo/prettier-config": "workspace:*",
    "@akimeo/typescript-config": "workspace:*",
    "@cloudflare/workers-types": "catalog:",
    "@testing-library/dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "concurrently": "catalog:",
    "jsdom": "catalog:",
    "prettier": "catalog:",
    "type-fest": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-checker": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:",
    "web-vitals": "catalog:"
  },
  "prettier": "@akimeo/prettier-config"
}
